{
    "Path": "jce_cn_cpu_miner$(if($DeviceGroup.Features.'64bit'){'64'}else{'32'}).exe",
    "Uri": "https://github.com/jceminer/cn_cpu_miner/raw/master/jce_cn_cpu_miner.windows.029c.zip",
    "SHA256": "7a6314d9b9983cb6799206248ac69b5b3afc81376d177503f442d55f0d446f29",
    "Type": "CPU",
    "Arguments": [
        "-o #Server#:#Port#",
        "-u #Login#",
        "-p #Password#",
        "$(if ($enableSSL){'--ssl'})",
        "$(if ($Nicehash){'--nicehash'})",
        "--low",
        "--any",
        "--mport #APIPort#",
        "#AlgorithmParameters#"
    ],
    "GenerateConfigFile": "config-#Algorithm#.txt",
    "PatternConfigFile": ".\\Patterns\\JCE-#Algorithm#-Pattern.txt",
    "API": "JCE",
    "APIPort": 4048,
    "Fee": "$(if($DeviceGroup.Features.aes -and $DeviceGroup.Features.'64bit'){0.015}else{0.03})",
    "SSL": true,
    "Algorithms": {
        "CryptoLightIPBC": "--variation 6 --auto",
        "CryptoLightV7": "--variation 4 --auto",
        "CryptoNightAlloy": "--variation 8 --auto",
        "CryptoNightArto": "--variation 10 --auto",
        "CryptoNightFast": "--variation 11 --auto",
        "CryptoNightHaven": "--variation 12 --auto",
        "CryptoNightHaven|dh": "--variation 12 -c config-CryptoNightHaven.txt",
        "CryptoNightHeavy": "--variation 5 --auto",
        "CryptoNightHeavy|dh": "--variation 5 -c config-CryptoNightHeavy.txt",
        "CryptoNightMkt": "--variation 9 --auto",
        "CryptoNightV7": "--variation 3 --auto",
        "CryptoNightXTL": "--variation 7 --auto"
    }
}