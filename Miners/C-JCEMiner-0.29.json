{
    "Path": "jce_cn_cpu_miner$(if($DeviceGroup.Features.'64bit'){'64'}else{'32'}).exe",
    "Uri": "https://github.com/jceminer/cn_cpu_miner/raw/master/jce_cn_cpu_miner.windows.029.zip",
    "SHA256": "ec6aa896c1395b5a336dbdcbe5f8e1eb8ef3e61a08b13684f1edba7ad7786264",
    "Type": "CPU",
    "Arguments": [
        "-o #Server#:#Port#",
        "-u #Login#",
        "-p #Password#",
        "$(if ($enableSSL){'--ssl'})",
        "$(if ($Nicehash){'--nicehash'})",
        "--stakjson",
        "--low",
        "--mport #APIPort#",
        "#AlgorithmParameters#"
    ],
    "GenerateConfigFile": "config-#Algorithm#.txt",
    "PatternConfigFile": ".\\Patterns\\JCE-#Algorithm#-Pattern.txt",
    "API": "XMRig",
    "APIPort": 4048,
    "Fee": "$(if($DeviceGroup.Features.aes -and $DeviceGroup.Features.'64bit'){0.015}else{0.03})",
    "SSL": true,
    "Algorithms": {
        "CryptoLightV7": "--variation 4 --any --auto",
        "CryptoNightV7": "--variation 3 --any --auto",
        "CryptoNightHeavy": "--variation 5 --any --auto",
        "CryptoNightHeavy|dh": "--variation 5 --any -c config-CryptoNightHeavy.txt",
        "CryptoLightIPBC": "--variation 6 --any --auto",
        "CryptoNightXTL": "--variation 7 --any --auto",
        "CryptoNightAlloy": "--variation 8 --any --auto",
        "CryptoNightMkt": "--variation 9 --any --auto",
        "CryptoNightArto": "--variation 10 --any --auto"
    }
}